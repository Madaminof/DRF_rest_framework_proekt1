<br />Program Gauss1; label 1,2,3,4,5; var a:array[1..10, 1..10] of real; b,x:array[1..10] of real; c,s:real; i,j,k,n:integer; begin readln(n); for i:=1 to n do begin for j:=1 to n do read(a[i,j]); readln(b[i]); end; k:=1; 3: i:=k+1; 2: c:=a[i,k]/a[k,k]; a[i,k]:=0; j:=k+1; 1: a[i,j]:=a[i,j]-c*a[k,j]; if j<n then begin j:=j+1; goto 1 end; b[i]:=b[i]-c*b[k]; if i<n then begin i:=i+1; goto 2 end; if k<n-1 then begin k:=k+1; goto 3 end; x[n]:=b[n]/a[n,n]; i:=n-1; 5: j:=i+1; s:=0; 4: s:=s+a[i,j]*x[j]; if j<n then begin j:=j+1; goto 4 end; x[i]:=(b[i]-s)/a[i,i]; if i>1 then begin i:=i-1; goto 5 end;  
